{
  "name": "triplepdigital",
  "description": "Just another Ghost theme made in Webflow and converted using the Udesly App.",
  "keywords": [
    "webflow",
    "udesly",
    "ghost-theme"
  ],
  "author": {
    "name": "Paul Pounder",
    "email": "paul@triplep.digital",
    "url": "https://www.triplep.digital"
  },
  "version": "1.0.0",
  "engines": {
    "ghost": ">=4.0.0"
  },
  "license": "MIT",
  "config": {
    "custom": {
      "content_api_key": {
        "type": "text",
        "default": ""
      }
    },
    "posts_per_page": 6,
    "image_sizes": {
      "xxs": {
        "width": 30
      },
      "xs": {
        "width": 100
      },
      "s": {
        "width": 300
      },
      "m": {
        "width": 600
      },
      "l": {
        "width": 1000
      },
      "xl": {
        "width": 2000
      }
    }
  }
}